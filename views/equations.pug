extends ./layout
block content
  .container.pt50.pb50
    h1.pb20 Equations
    .pb20
      p
        strong Material Derivative
      .pl50 $\frac{Df}{Dt} = \frac{\partial f}{\partial t} + \vec{v} \cdot \nabla f$
    .pb20
      p
        strong Conservation of Mass
      .pl50 $\frac{D \rho}{Dt} + \nabla \cdot \rho \vec{v} = 0$
    .pb20
      p
        strong Navier-Stokes Equations
      .pl50 $\frac{\partial \vec{v}}{\partial t} + (\vec{v} \cdot \nabla)\vec{v} = -\frac{\nabla p}{\rho} + \nu \nabla^{2} \vec{v} + g \nabla z$
    .pb20
      p
        strong Euler Equations
      .pl50 $\frac{\partial \vec{v}}{\partial t} + (\vec{v} \cdot \nabla)\vec{v} = -\frac{\nabla p}{\rho} + g \nabla z$
    .pb20
      p
        strong Kelvin's Theorem
      .pl50 $\frac{d \Gamma}{dt} = 0$
    .pb20
      p
        strong Bernoulli's Equation
      .pl50 $\rho \frac{\partial \phi}{\partial t} + \rho\!\left(\frac{| \vec{v} |^{2}}{2}\right) + p + \rho gz = constant$
    .pb20
      p
        strong Incompressible Flow
      .pl50 $\nabla \cdot \vec{v} = 0$
    .pb20
      p
        strong Irrotational Flow
      .pl50 $\nabla \times \vec{v} = 0$
    .pb20
      p
        strong Laplace's Equation
      .pl50 $\nabla^{2} \phi = 0$
    .pb20
      p
        strong Point Source/Sink
      .pl50
        .mb5 $\phi = \frac{m}{2\pi}\ln(r)$
        .mb5 $\psi = \frac{m}{2\pi}\theta$
        .mb5 $\vec{v} = \frac{m}{2\pi r}\hat{r}$
    .pb20
      p
        strong Point Vortex
      .pl50
        .mb5 $\phi = \frac{\Gamma}{2\pi}\theta$
        .mb5 $\psi = -\frac{\Gamma}{2\pi}\ln(r)$
        .mb5 $\vec{v} = \frac{\Gamma}{2\pi r}\hat{\theta}$
    .pb20
      p
        strong Dipole
      .pl50
        .mb5 $\phi = -\frac{\mu}{2\pi} \frac{(x - x_o)\cos(\alpha)\ +\ (y - y_o)\sin(\alpha)}{(x - x_o)^{2}\ +\ (y - y_o)^{2}}$
        .mb5 $\psi = \frac{\mu}{2\pi} \frac{(x - x_o)\sin(\alpha)\ +\ (y - y_o)\cos(\alpha)}{(x - x_o)^{2}\ +\ (y - y_o)^{2}}$
        .mb5 $v_x = -\frac{\mu}{2\pi}\frac{\left((x - x_o)^{2}\ +\ (y - y_o)^{2}\right)\!\cos(\alpha)\ -\ 2(x - x_o)\left((x - x_o)\cos(\alpha)\ +\ (y - y_o)\sin(\alpha)\right)}{\left((x - x_o)^{2}\ +\ (y - y_o)^{2}\right)^{2}}$
        .mb5 $v_y = -\frac{\mu}{2\pi}\frac{\left((x - x_o)^{2}\ +\ (y - y_o)^{2}\right)\!\sin(\alpha)\ -\ 2(y - y_o)\left((x - x_o)\cos(\alpha)\ +\ (y - y_o)\sin(\alpha)\right)}{\left((x - x_o)^{2}\ +\ (y - y_o)^{2}\right)^{2}}$
    .pb20
      p
        strong Corner Flow
      .pl50
        .mb5 $\phi = \beta r^{\alpha}\cos(\alpha(\theta - \theta_o))$
        .mb5 $\psi = \beta r^{\alpha}\sin(\alpha(\theta - \theta_o))$
        .mb5 $v_{r} = \frac{\partial \phi}{\partial r} = \beta\alpha r^{\alpha - 1}\cos(\alpha(\theta - \theta_o))$
        .mb5 $v_{\theta} = \frac{1}{r}\frac{\partial \phi}{\partial\theta} = -\beta\alpha r^{\alpha - 1}\sin(\alpha(\theta - \theta_o))$
